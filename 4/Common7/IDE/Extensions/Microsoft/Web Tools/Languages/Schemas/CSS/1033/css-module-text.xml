<CssModule name="Text" standard-reference="https://www.w3.org/TR/css-text-3/" status="CRD" pub-date="2021-04-22">

    <CssProperty _locID="text-transform" _locAttrData="description" restriction="enum" syntax="h1 { $(name): capitalize; }" description="Controls capitalization effects of an element's text." standard-reference="https://www.w3.org/TR/css-text-3/#text-transform-property">

        <entry _locID="text-transform_none" _locAttrData="description" value="none" description="No capitalization effects." />
        <entry _locID="text-transform_capitalize" _locAttrData="description" value="capitalize" description="Puts all words in titlecase." />
        <entry _locID="text-transform_uppercase" _locAttrData="description" value="uppercase" description="Puts all characters of each word in uppercase." />
        <entry _locID="text-transform_lowercase" _locAttrData="description" value="lowercase" description="Puts all characters of each word in lowercase." />
        <entry _locID="text-transform_fullwidth" _locAttrData="description" value="full-width" version="3.0" browsers="none" description="Puts all characters in fullwidth form. If the character does not have corresponding fullwidth form, it is left as is." />
        <entry _locID="text-transform_fullsize-kana" _locAttrData="description" value="fullsize-kana" version="3.0" browsers="none" description="Converts all small Kana characters to normal Kana. This value is typically used for ruby annotation text." />
    </CssProperty>

    <CssProperty _locID="white-space" _locAttrData="description" version="1.0" restriction="enum" syntax="section { $(name): nowrap; }" description="Shorthand property for the 'white-space-collapsing' and 'text-wrap' properties." standard-reference="https://www.w3.org/TR/css-text-3/#white-space-property">

        <entry _locID="white-space_normal" _locAttrData="description" value="normal" description="This value directs user agents to collapse sequences of white space into a single character (or in some cases, no character)." />
        <entry _locID="white-space_pre" _locAttrData="description" value="pre" description="This value prevents user agents from collapsing sequences of white space. Segment breaks such as line feeds are preserved as forced line breaks." />
        <entry _locID="white-space_nowrap" _locAttrData="description" value="nowrap" description="Like normal, this value collapses white space; but like pre, it does not allow wrapping." />
        <entry _locID="white-space_pre-wrap" _locAttrData="description" value="pre-wrap" description="Like pre, this value preserves white space; but like normal, it allows wrapping." />
        <entry _locID="white-space_break-spaces" _locAttrData="description" value="break-spaces" description="The behavior is identical to that of pre-wrap, except that any sequence of preserved white space or other space separators always takes up space, including at the end of the line, and a line breaking opportunity exists after every preserved white space character and after every other space separator (including between adjacent spaces)." />
        <entry _locID="white-space_pre-line" _locAttrData="description" value="pre-line" description="Like normal, this value collapses consecutive white space characters and allows wrapping, but it preserves segment breaks in the source as forced line breaks." />
    </CssProperty>

    <CssProperty _locID="tab-size" _locAttrData="description" version="3.0" browsers="none" restriction="integer, length" syntax="div { $(name): 4; }" description="This property determines the width of the tab character (U+0009), in space characters (U+0020), when rendered" standard-reference="https://www.w3.org/TR/css-text-3/#tab-size" />

    <CssProperty _locID="word-break" _locAttrData="description" version="3.0" browsers="C,FF15,IE5,S3" restriction="enum" syntax="p.album { $(name): break-all; }" description="Specifies line break opportunities for non-CJK scripts." standard-reference="https://www.w3.org/TR/css-text-3/#word-break-property">

        <entry _locID="word-break_normal" _locAttrData="description" value="normal" description="Breaks non-CJK scripts according to their own rules." />
        <entry _locID="word-break_break-all" _locAttrData="description" value="break-all" description="Lines may break between any two grapheme clusters for non-CJK scripts. This option is used mostly in a context where the text is predominantly using CJK characters with few non-CJK excerpts and it is desired that the text be better distributed on each line." />
        <entry _locID="word-break_keep-all" _locAttrData="description" value="keep-all" description="Block characters can no longer create implied break points. Otherwise this option is equivalent to 'normal'. This option is mostly used where the presence of word separator characters still creates line-breaking opportunities, as in Korean." />
        <entry value="break-word" />
    </CssProperty>

    <CssProperty _locID="line-break" _locAttrData="description" version="3.0" browsers="none" restriction="enum" syntax="p { $(name): normal; }" description="Specifies what set of line breaking restrictions are in effect within the element." standard-reference="https://www.w3.org/TR/css-text-3/#line-break-property">

        <entry _locID="line-break_auto" _locAttrData="description" value="auto" description="The UA determines the set of line-breaking restrictions to use for CJK scripts, and it may vary the restrictions based on the length of the line; e.g., use a less restrictive set of line-break rules for short lines." />
        <entry _locID="line-break_loose" _locAttrData="description" value="loose" description="Breaks text using the least restrictive set of line-breaking rules. Typically used for short lines, such as in newspapers." />
        <entry _locID="line-break_normal" _locAttrData="description" value="normal" description="Breaks text using the most common set of line-breaking rules." />
        <entry _locID="line-break_strict" _locAttrData="description" value="strict" description="Breaks CJK scripts using a more restrictive set of line-breaking rules than 'normal'." />
        <entry _locID="line-break_anywhere" _locAttrData="description" value="anywhere" description="There is a soft wrap opportunity around every typographic character unit, including around any punctuation character or preserved white spaces, or in the middle of words, disregarding any prohibition against line breaks, even those introduced by characters with the GL, WJ, or ZWJ line breaking classes or mandated by the word-break property." />
    </CssProperty>

    <CssProperty _locID="hyphens" _locAttrData="description" version="3.0" browsers="none" type="hyphens" restriction="enum" description="This property controls whether hyphenation is allowed to create more break opportunities within a line of text." syntax="div { $(name): manual; }" standard-reference="https://www.w3.org/TR/css-text-3/#hyphenation" />

    <CssPropertyValue type="hyphens">
        <entry _locID="hyphens_none" _locAttrData="description" value="none" description="Words are not broken at line breaks, even if characters inside the word suggest line break points." />
        <entry _locID="hyphens_manuel" _locAttrData="description" value="manual" description="Words are only broken at line breaks where there are characters inside the word that suggest line break opportunities" />
        <entry _locID="hyphens_auto" _locAttrData="description" value="auto" description="Conditional hyphenation characters inside a word, if present, take priority over automatic resources when determining hyphenation points within the word." />
    </CssPropertyValue>

    <CssProperty _locID="overflow-wrap" _locAttrData="description" version="3.0" browsers="none" restriction="enum" type="word-wrap" syntax="div { $(name): break-word; }" description="This property specifies whether the UA may break within a word to prevent overflow when an otherwise-unbreakable string is too long to fit within the line box." standard-reference="https://www.w3.org/TR/css-text-3/#overflow-wrap-property" />

    <CssProperty _locID="word-wrap" _locAttrData="description" version="2.0" restriction="enum" type="word-wrap" syntax="p { $(name): break-word; }" description="Specifies whether the UA may break within a word to prevent overflow when an otherwise-unbreakable string is too long to fit." standard-reference="https://www.w3.org/TR/css-text-3/#overflow-wrap-property" />

    <CssPropertyValue type="word-wrap">
        <entry _locID="word-wrap_normal" _locAttrData="description" value="normal" description="Lines may break only at allowed break points." />
        <entry _locID="word-wrap_break-word" _locAttrData="description" value="break-word" description="An unbreakable 'word' may be broken at an arbitrary point if there are no otherwise-acceptable break points in the line." />
        <entry _locID="word-wrap_anywhere" _locAttrData="description" value="anywhere" description="An otherwise unbreakable sequence of characters may be broken at an arbitrary point if there are no otherwise-acceptable break points in the line." />
    </CssPropertyValue>

    <CssPropertyValue type="text-align">
        <entry _locID="text-align_start" _locAttrData="description" value="start" version="3.0" browsers="none" description="The inline contents are aligned to the start edge of the line box." />
        <entry _locID="text-align_end" _locAttrData="description" value="end" version="3.0" browsers="none" description="The inline contents are aligned to the end edge of the line box." />
        <entry _locID="text-align_left" _locAttrData="description" value="left" description="The inline contents are aligned to the left edge of the line box. In vertical text, 'left' aligns to the edge of the line box that would be the start edge for left-to-right text." />
        <entry _locID="text-align_right" _locAttrData="description" value="right" description="The inline contents are aligned to the right edge of the line box. In vertical text, 'right' aligns to the edge of the line box that would be the end edge for left-to-right text." />
        <entry _locID="text-align_center" _locAttrData="description" value="center" description="The inline contents are centered within the line box." />
        <entry _locID="text-align_justify" _locAttrData="description" value="justify" description="The text is justified according to the method specified by the 'text-justify' property." />
        <entry _locID="text-align_match-parent" _locAttrData="description" value="match-parent" version="3.0" browsers="none" description="This value behaves the same as 'inherit' except that an inherited value of 'start' or 'end' is calculated against its parent's 'direction' value." />
    </CssPropertyValue>

    <CssProperty _locID="text-align" _locAttrData="description" version="1.0" restriction="enum" type="text-align" browsers="all" syntax="h2 { $(name): center; }" description="Describes how inline contents of a block are horizontally aligned if the contents do not completely fill the line box." standard-reference="https://www.w3.org/TR/css-text-3/#text-align-property">

        <entry _locID="text-align_justify-all" _locAttrData="description" value="justify-all" description="Sets both text-align-all and text-align-last to justify, forcing the last line to justify as well." />
    </CssProperty>

    <!-- Spec defines text-align-all but also states that authors should use text-align instead. -->

    <CssProperty _locID="text-align-last" _locAttrData="description" version="3.0" browsers="FF12,IE5" restriction="enum" type="text-align" syntax="div { $(name): right; }" description="Describes how the last line of a block or a line right before a forced line break is aligned when 'text-align' is set to 'justify'." standard-reference="https://www.w3.org/TR/css-text-3/#text-align-last-property">

        <entry value="auto" version="3.0" />
    </CssProperty>

    <CssProperty _locID="text-justify" _locAttrData="description" version="3.0" browsers="IE5.5" restriction="enum" description="Selects the justification algorithm used when 'text-align' is set to 'justify'. The property applies to block containers, but the UA may (but is not required to) also support it on inline elements." syntax="div { $(name): inter-word; }" standard-reference="https://www.w3.org/TR/css-text-3/#text-justify-property">

        <entry _locID="text-justify_none" _locAttrData="description" value="none" browsers="none" description="Justification is disabled." />
        <entry _locID="text-justify_auto" _locAttrData="description" value="auto" description="The UA determines the justification algorithm to follow, based on a balance between performance and adequate presentation quality." />
        <entry _locID="text-justify_inter-word" _locAttrData="description" value="inter-word" description="Justification primarily changes spacing at word separators. This value is typically used for languages that separate words using spaces, like English or (sometimes) Korean." />
        <entry _locID="text-justify_inter-character" _locAttrData="description" value="inter-character" description="Justification adjusts spacing between each pair of adjacent typographic character units (effectively varying the used letter-spacing on the line). This value is sometimes used in East Asian systems such as Japanese." />
    </CssProperty>

    <CssProperty _locID="word-spacing" _locAttrData="description" version="1.0" restriction="length" description="Specifies the minimum, maximum, and optimal spacing between words." syntax="article { $(name): 3px; }" standard-reference="https://www.w3.org/TR/css-text-3/#word-spacing0">

        <entry _locID="word-spacing_normal" _locAttrData="description" value="normal" description="The normal inter-word space, as defined by the current font and/or the user agent." />
    </CssProperty>

    <CssProperty _locID="letter-spacing" _locAttrData="description" version="1.0" restriction="length" description="Specifies the minimum, maximum, and optimal spacing between grapheme clusters." syntax="h2 { $(name): 2px; }" standard-reference="https://www.w3.org/TR/css-text-3/#letter-spacing0">

        <entry _locID="letter-spacing_normal" _locAttrData="description" value="normal" description="The spacing is the normal spacing for the current font. It is typically zero-length." />
    </CssProperty>

    <CssProperty _locID="text-indent" _locAttrData="description" version="1.0" restriction="percentage, length" description="Specifies the indentation applied to lines of inline content in a block. The indentation only affects the first line of inline content in the block unless the 'hanging' keyword is specified, in which case it affects all lines except the first." syntax="li { $(name): 5px; }" standard-reference="https://www.w3.org/TR/css-text-3/#text-indent0">

        <entry _locID="text-indent_hanging" _locAttrData="description" value="hanging" version="3.0" browsers="none" description="Inverts which lines are affected." />
        <entry _locID="text-indent_each-line" _locAttrData="description" value="each-line" version="3.0" browsers="none" description="Indentation affects the first line of the block container as well as each line after a forced line break, but does not affect lines after a text wrap break." />
    </CssProperty>

    <CssProperty _locID="hanging-punctuation" _locAttrData="description" version="3.0" browsers="none" restriction="enum" syntax="p { $(name): first; }" description="Determines whether a punctuation mark, if one is present, may be placed outside the content area at the start or at the end of a full line of text." standard-reference="https://www.w3.org/TR/css-text-3/#hanging-punctuation0">

        <!-- Watch for changes! Reference says:
          Need to work on the description. Add Unicode character classes.
          Cover indentation as well. Check for Western use-cases. Add hyphens value?' -->

        <entry _locID="hanging-punctuation_none" _locAttrData="description" value="none" description="No characters can hang." />
        <entry _locID="hanging-punctuation_first" _locAttrData="description" value="first" description="Punctuation (specifically, opening brackets and quotes) may hang outside the start edge of the first line." />
        <entry _locID="hanging-punctuation_last" _locAttrData="description" value="last" description="A closing bracket or quote at the end of the last formatted line of an element hangs. This applies to all characters in the Unicode categories Pe, Pf, Pi." />
        <entry _locID="hanging-punctuation_allow-end" _locAttrData="description" value="allow-end" description="Punctuation (specifically, stops and commas) may hang outside the end edge of all lines if the punctuation does not otherwise fit prior to justification." />
        <entry _locID="hanging-punctuation_force-end" _locAttrData="description" value="force-end" description="Punctuation (specifically, stops and commas) may hang outside the end edge of all lines. If justification is enabled on this line, then it will force the punctuation to hang." />
    </CssProperty>

    <!-- From CSS Mobile Text Size Adustment module, spec still unpublished -->
    <CssProperty _locID="text-size-adjust" _locAttrData="description" version="3.0" browsers="none" restriction="enum, percentage" syntax="body { $(name): 150%; }" description="Specifies a size adjustment for displaying text content in mobile browsers." standard-reference="http://dev.w3.org/csswg/css-size-adjust/">

        <entry value="auto" />
        <entry value="none" />
    </CssProperty>

