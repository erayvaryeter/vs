{
  "feature": {
    "id": "storage.emulator"
  },
  "requirements": [
    "dockerDesktop"
  ],
  "editSteps": [
    "Containers",
    "Connection",
    "DependencySummary"
  ],
  "secretStores": [
    "secrets.user",
    "secrets.keyVault",
    "none"
  ],
  "properties": {
    "ConnectionValue": {
      "value": "UseDevelopmentStorage=true"
    },
    "BlobEndpoint": {
      "value": "UseDevelopmentStorage=true"
    },
    "QueueEndpoint": {
      "value": "UseDevelopmentStorage=true"
    },
    "SatellitesToGenerate": {
      "value": "docker-compose"
    },
    "containerName": {
      "value": "azurite"
    },
    "containerImage": {
      "value": "mcr.microsoft.com/azure-storage/azurite"
    },
    "containerPorts": {
      "value": "10000:10000,10001:10001,10002:10002"
    }
  },
  "persistencePreprocessors": [
    "SecretStoreResourceId"
  ],
  "persistenceProperties": {
    "connectionId": {
      "isBase": true
    },
    "dynamicId": {
      "isBase": true
    },
    "ignored": {
      "isBase": true
    },
    "resourceId": {
      "parametrizer": "resourceId"
    },
    "secretStore": {},
    "serviceConnectorResourceId": {
      "parametrizer": "resourceId"
    },
    "suggestion": {
      "isBase": true
    },
    "type": {
      "isBase": true
    },
    "containerName": {},
    "containerImage": {},
    "containerPorts": {},
    "group": {
      "isBase": true
    }
  },
  "satellites": [
    {
      "id": "docker-compose",
      "kind": "shadow",
      "generator": "static",
      "fileExtension": ".docker-compose.yml",
      "model": {
        "templatePath": "path:#/dependencyTypes/configs/storage/docker-compose.yml"
      }
    }
  ]
}